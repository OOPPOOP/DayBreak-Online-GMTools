	<SGMCommandTemplate>
		<Name>物品/金币补偿[系统邮件]</Name>
		<ScriptTemplate>GM:SendMailToPlayer('%RoleName%','{0}','{1}',0,{2},0, {3}, {4}, {5},-1,-1)</ScriptTemplate>
		<PriObjPath>masterweb/gm/advanceop</PriObjPath>
		<ExecType>1</ExecType>
		<Params>
			<Name>邮件标题</Name>
			<Desc>不能为空且不要出现单引号和双引号，最好不要超过10个字。</Desc>
			<DefaultValue>物品补偿</DefaultValue>
		</Params>
		<Params>
			<Name>邮件内容</Name>
			<Desc>不能为空且不要出现单引号和双引号。</Desc>
			<DefaultValue>rt</DefaultValue>
		</Params>
		<Params>
			<Name>金币</Name>
			<Desc></Desc>
			<DefaultValue>0</DefaultValue>
		</Params>
		<Params>
			<Name>物品ID</Name>
			<Desc>不能为空，并注意ID的格式。</Desc>
			<DefaultValue>0,0,0,0</DefaultValue>
		</Params>
		<Params>
			<Name>物品数量</Name>
			<Desc>每封邮件最多发6组物品，注意物品每组的最大叠加数。</Desc>
			<DefaultValue>1</DefaultValue>
		</Params>
		<Params>
			<Name>是否绑定</Name>
			<Desc>1为绑定，0为非绑定。</Desc>
			<DefaultValue>1</DefaultValue>
		</Params>
	</SGMCommandTemplate>
