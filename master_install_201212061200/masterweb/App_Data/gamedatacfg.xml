<?xml version="1.0" encoding="utf-8"?>
<ArrayOfSQueryDataConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<SQueryDataConfig>
		<Name>AuctionInfo</Name>
		<Desc>拍卖行信息</Desc>
		<PriObjPath>masterweb/gm/currdata</PriObjPath>
		<SqlTextFormat>SELECT `Id`, `SellerName`, `GoodsName`, `StartTime`, `FinishTime`, `CurrentPrice`, `PassPrice`, `BuyerName` FROM `auction` ORDER BY {0} {1} LIMIT {2}, {3};</SqlTextFormat>
		<SqlTotalTextFormat>SELECT COUNT(*) FROM `auction` {0};</SqlTotalTextFormat>
		<OrderField>Id</OrderField>
		<OrderAsc>false</OrderAsc>
		<PriObjPath></PriObjPath>
		<Fields>
			<Name>Id</Name>
			<Desc>Id</Desc>
			<Type>INT</Type>
			<Width>50</Width>
		</Fields>
		<Fields>
			<Name>SellerName</Name>
			<Desc>出售者</Desc>
			<Type>VARBINARY</Type>
			<Width>110</Width>
		</Fields>
		<Fields>
			<Name>GoodsName</Name>
			<Desc>商品名</Desc>
			<Type>VARBINARY</Type>
			<Width>140</Width>
		</Fields>
		<Fields>
			<Name>StartTime</Name>
			<Desc>开始时间</Desc>
			<Type>DATETIME</Type>
			<Width>140</Width>
		</Fields>
		<Fields>
			<Name>FinishTime</Name>
			<Desc>结束时间</Desc>
			<Type>DATETIME</Type>
			<Width>140</Width>
		</Fields>
		<Fields>
			<Name>CurrentPrice</Name>
			<Desc>当前价</Desc>
			<Type>INT</Type>
			<Width>80</Width>
		</Fields>
		<Fields>
			<Name>PassPrice</Name>
			<Desc>一口价</Desc>
			<Type>INT</Type>
			<Width>80</Width>
		</Fields>
		<Fields>
			<Name>BuyerName</Name>
			<Desc>竞拍者</Desc>
			<Type>VARBINARY</Type>
			<Width>110</Width>
		</Fields>
	</SQueryDataConfig>

