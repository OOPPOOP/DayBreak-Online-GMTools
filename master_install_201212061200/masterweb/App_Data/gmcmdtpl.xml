<?xml version="1.0" encoding="utf-8"?>
<ArrayOfSGMCommandTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<SGMCommandTemplate>
		<Name>传送[布鲁斯镇]</Name>
		<ScriptTemplate>GM:PlayerTrapTo('%RoleName%',3,3)</ScriptTemplate>
		<PriObjPath>masterweb/gm/commonop</PriObjPath>
		<ExecType>0</ExecType>
	</SGMCommandTemplate>
	<SGMCommandTemplate>
		<Name>传送[福克镇]</Name>
		<ScriptTemplate>GM:PlayerTrapTo('%RoleName%',7,36)</ScriptTemplate>
		<PriObjPath>masterweb/gm/commonop</PriObjPath>
		<ExecType>0</ExecType>
	</SGMCommandTemplate>
	<SGMCommandTemplate>
		<Name>传送[洛亚王城]</Name>
		<ScriptTemplate>GM:PlayerTrapTo('%RoleName%',30,23)</ScriptTemplate>
		<PriObjPath>masterweb/gm/commonop</PriObjPath>
		<ExecType>0</ExecType>
	</SGMCommandTemplate>
	<SGMCommandTemplate>
		<Name>修复K[技能]、I[公会]、L[强化]</Name>
		<ScriptTemplate>CPlayerMgr:PlayerExc("%RoleName%","Me:ChgNewRoleFlags(4,1)")&#10;CPlayerMgr:PlayerExc("%RoleName%","Me:ChgNewRoleFlags(8,1)")&#10;CPlayerMgr:PlayerExc("%RoleName%","Me:ChgNewRoleFlags(16,1)")</ScriptTemplate>
		<PriObjPath>masterweb/gm/commonop</PriObjPath>
		<ExecType>0</ExecType>
	</SGMCommandTemplate>
	<SGMCommandTemplate>
		<Name>强制存盘</Name>
		<ScriptTemplate>GM:ForceSaveAll('%RoleName%')</ScriptTemplate>
		<PriObjPath>masterweb/gm/commonop</PriObjPath>
		<ExecType>1</ExecType>
	</SGMCommandTemplate>
	<SGMCommandTemplate>
		<Name>物品/金币补偿[系统邮件]</Name>
		<ScriptTemplate>GM:SendMailToPlayer('%RoleName%','{0}','{1}',0,{2},0, {3}, {4}, {5},-1,-1)</ScriptTemplate>
		<PriObjPath>masterweb/gm/advanceop</PriObjPath>
		<ExecType>1</ExecType>
		<Params>
			<Name>邮件标题</Name>
			<Desc>不能为空且不要出现单引号和双引号，最好不要超过10个字。</Desc>
			<DefaultValue>物品补偿</DefaultValue>
		</Params>
		<Params>
			<Name>邮件内容</Name>
			<Desc>不能为空且不要出现单引号和双引号。</Desc>
			<DefaultValue>rt</DefaultValue>
		</Params>
		<Params>
			<Name>金币</Name>
			<Desc></Desc>
			<DefaultValue>0</DefaultValue>
		</Params>
		<Params>
			<Name>物品ID</Name>
			<Desc>不能为空，并注意ID的格式。</Desc>
			<DefaultValue>0,0,0,0</DefaultValue>
		</Params>
		<Params>
			<Name>物品数量</Name>
			<Desc>每封邮件最多发6组物品，注意物品每组的最大叠加数。</Desc>
			<DefaultValue>0</DefaultValue>
		</Params>
		<Params>
			<Name>是否绑定</Name>
			<Desc>1为绑定，0为非绑定。</Desc>
			<DefaultValue>1</DefaultValue>
		</Params>
	</SGMCommandTemplate>
	<SGMCommandTemplate>
		<Name>通用离线GM脚本</Name>
		<ScriptTemplate>{0}</ScriptTemplate>
		<PriObjPath>masterweb/operation/script</PriObjPath>
		<ExecType>0</ExecType>
		<Params>
			<Name>脚本</Name>
			<Desc />
		</Params>
	</SGMCommandTemplate>
</ArrayOfSGMCommandTemplate>
